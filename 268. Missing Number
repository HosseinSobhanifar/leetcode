class Solution(object):
    def missingNumber(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        nums.sort()
        length = len(nums)
        first = 0
        end = length - 1
        while True:
            mid = (end - first) // 2 + first

            if first == end:
                if nums[first] == first:
                    return(length)
                else:
                    return(first)

            if nums[mid] == mid:
                first = mid + 1

            else:
                end = mid


